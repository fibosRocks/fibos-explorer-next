{
  "name": "fibos-explorer-next",
  "version": "1.0.0",
  "private": true,
  "description": "FIBOS Blockchain Explorer - Next.js Rewrite",
  "packageManager": "bun@1.3.6",
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3100",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "test": "bun test",
